@using Claro.Foundation.Assets.Services
<!DOCTYPE html>
<html lang="en">

<head>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-119414326-6"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-119414326-6');
    </script>
    <meta charset="utf-8">
    <title>@Html.Sitecore().Field("Page Title", new { DisableWebEdit = true })</title>
    <meta name="description" content="@Html.Sitecore().Field("Meta Description", new { DisableWebEdit = true })">
    <meta name="keywords" content="@Html.Sitecore().Field("Meta Keyword", new { DisableWebEdit = true })">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    @Html.Sitecore().Placeholder("social-metadata")
    <link rel="shorcut icon" href="/images/favicon.ico" />
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/all.css">
    <link rel="stylesheet" href="/css/claro_style.css">
    <link rel="stylesheet" href="/css/heander_menu.css">
    <link rel="stylesheet" href="/css/owl.carousel.css">
    <link rel="stylesheet" href="/css/owl.theme.default.css">
</head>

<body>
    <header>
        <div class="overlay"></div>
        @Html.Sitecore().Placeholder("header-content")
    </header>
    @Html.Sitecore().Placeholder("main-content")
    <!-- Scripts -->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/jquery.ellipsis.min.js"></script>
    <script src="/js/wow.js"></script>

    <script src="/js/core.js"></script>
    <script src="/js/owl.carousel.js"></script>
    <script src="/js/common.js"></script>
    @AssetsService.Current.RenderResource()
</body>

</html>
